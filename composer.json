{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/formandsystem/framework"
		},
		{
			"type": "vcs",
			"url": "https://github.com/formandsystem/oauth2-server-laravel"
		}
	],
	"require": {
		"laravel/framework": "~5.0",
		"lucadegasperi/oauth2-server-laravel": "~4.0@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.3@stable",
		"phpspec/phpspec": "~2.1@dev",
		"fzaninotto/faker": "~1.5@dev",
		"mockery/mockery": "~0.9@dev"
	},
	"autoload": {
		"classmap": [
			"database",
			"tests/TestCase.php",
			"tests"
		],
		"psr-4": {
			"Formandsystemapi\\": "app/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan route:scan",
			"php artisan event:scan",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
